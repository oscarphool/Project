<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintVertical_bias="0.729"
            android:layout_height="100dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/button2" />
        <Constraint
            android:layout_width="3dp"
            android:layout_height="3dp"
            android:id="@+id/imageView2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.998"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp" />
        <Constraint
            motion:layout_editor_absoluteY="696dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="53dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView6" />
        <Constraint
            motion:layout_editor_absoluteY="728dp"
            android:layout_height="2dp"
            motion:layout_editor_absoluteX="7dp"
            android:layout_width="21dp"
            android:id="@+id/textView8" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_constraintVertical_bias="0.031"
            android:layout_height="220dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="186dp"
            android:id="@+id/la_centerAnimation" />
        <Constraint
            motion:layout_constraintVertical_bias="0.124"
            android:layout_height="103dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.898"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="253dp"
            android:id="@+id/button2" />
        <Constraint
            android:layout_height="404dp"
            android:layout_width="200dp"
            android:id="@+id/imageView2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.902"
            motion:layout_constraintHorizontal_bias="0.856"
            motion:layout_editor_absoluteY="290dp" />
        <Constraint
            motion:layout_editor_absoluteY="345dp"
            android:layout_height="40dp"
            motion:layout_editor_absoluteX="22dp"
            android:layout_width="120dp"
            android:id="@+id/textView6" />
        <Constraint
            android:layout_height="260dp"
            motion:layout_editor_absoluteX="7dp"
            android:layout_width="150dp"
            motion:layout_constraintTop_toBottomOf="@+id/la_centerAnimation"
            android:id="@+id/textView8"
            android:layout_marginTop="136dp" />
    </ConstraintSet>
</MotionScene>